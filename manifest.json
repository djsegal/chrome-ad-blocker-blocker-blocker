{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "ad-blocker-blocker-blocker",
	"version": "1.0",
	"description": "Add this app to your Chrome browser and block ad blocker blockers",
  "permissions": [
    "http://*.google.com/", "http://*.com/", "tabs"
  ],
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["jquery.min.js", "content_script.js"],
			"run_at": "document_end"
		}
	],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Block ad blocker blockers",
    "default_popup": "popup.html"
  }
}
